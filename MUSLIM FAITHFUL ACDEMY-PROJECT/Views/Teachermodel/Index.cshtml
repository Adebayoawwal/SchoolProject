@model List<MUSLIM_FAITHFUL_ACDEMY_PROJECT.Models.TeacherModel>

@{
}
<h1>Teacher</h1>
<table class="table">
	<thead>
		<tr>
			<th>TeacherName</th>
			<th>Email</th>
			<th>ClassType</th>
			<th>School Name</th>
			<th> </th>
		</tr>
	</thead>
	<tbody>

		@foreach (var student in Model)
		{
			<tr>
				<td>@student.TeacherName</td>
				<td>@student.Email</td>
				<td>@student.ClassType</td>
				<td><button type="submit" asp-controller="Student" asp-action="Edit">Edit</button></td>
			</tr>
		}
	</tbody>
</table>
